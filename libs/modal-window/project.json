{
  "name": "modal-window",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/modal-window/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/modal-window/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/libs/modal-window"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "configure-react-library": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "nx g @fi-sci/fi-sci-plugin:configure-react-library modal-window",
            "color": "blue"
          }
        ]
      }
    }
  }
}
